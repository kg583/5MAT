INIT {
    PRINA "Beginning tests"
    LOOP 3 { PRINC '.' }
    PRFF

    IFNR 9 [ FRESH 9 ]

    CASES {
        "FOOF"       { CREQ '0', '1' }
        "FLATULENCE" { BRINC 4, $R, 6 }
    }
}

DO {
    PRINL 1, -2, $R, '4', $V

    LOOP 0 {
        PRINA "LOOP 0 test failed!"
    }

    {
        PRINA "This ↓ "
        {
            PRINC $V
            BACK
        }

        PRINA "Is this ↓ "
        [
            PRINC ?V
        ]
    }

    LOWER { PRINA "foo bar" }
    UPPER { PRINA "FOO BAR" }
    TITLE { PRINA "Foo Bar" }
    TITLE 1 { PRINA "Foo bar" }

    JUST 9, NIL, 7, ?V {
        OVER -6, 5 { SKIPF }
        { BACKF }
        [ GOTOF ]
        { COPYR }
    }

    IFFF {
        PRFF
        CRFF
        BRFF
    }

    COPYR 5
    COPYF 1

    CASER [
        0 [ TABR ]
        DEFAULT { TABA }
    ]

    CASEV {
        'F' { |~:p~*| }
    }

    PREQA $R, 5, ?3
    PRGTC $V, 'Q', '\xA4'

    PRINA "Behold, the most awful string in the world: `$R` \"$1\" '$V' %N% +M+ \n\tWoohoo!"
}