INIT {
    PRINA "Beginning tests"
    LOOP 3 {
        PRINC '.
    }
    PRFF
}

DO {
    PRINL 1, 2, $R, '4, $V

    LOOP 0 {
        PRINA "LOOP 0 test failed!"
    }

    {
        PRINA "This ↓ "
        {
            PRINC $V
            BACK
        }

        PRINA "Is this ↓ "
        {
            PRINC ?V
        }
    }

    LOWER { PRINA "foo bar" }
    UPPER { PRINA "FOO BAR" }
    TITLE { PRINA "Foo Bar" }
    TITLE 1 { PRINA "Foo bar" }

    JUST 9, NIL, 7, ?V {
        OVER 6, 5 { SKIPS }
        { BACKS }
        { GOTOS }
        { COPYS }
    }

    IFFF {
        PRFF
        CRFF
        BRFF
    }

    COPYR 5
}